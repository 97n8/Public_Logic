{
  "name": "publiclogic-os",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "vite",
    "dev:components": "npm --prefix \"PublicLogic OS Component Library (4)\" run dev",
    "dev:all": "concurrently -n app,components -c blue,green \"npm run dev:app\" \"npm run dev:components\"",
    "build": "tsc -b && vite build",
    "build:app": "tsc -b && vite build",
    "build:components": "npm --prefix \"PublicLogic OS Component Library (4)\" run build",
    "lint": "eslint .",
    "lint:app": "eslint .",
    "verify": "npm run lint:app && npm run build:app && npm run build:components",
    "install:components": "npm --prefix \"PublicLogic OS Component Library (4)\" install",
    "install:all": "npm install && npm run install:components",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
